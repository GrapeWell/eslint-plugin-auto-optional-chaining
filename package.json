{
  "name": "eslint-plugin-auto-optional-chaining",
  "version": "0.0.2",
  "description": "ðŸ›  Automatically enforce optional chaining (?.) to prevent null/undefined errors in JavaScript/TypeScript.",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "optional-chaining",
    "null-safety",
    "type-safe",
    "javascript",
    "typescript"
  ],
  "author": "saxibulidala",
  "repository": {
    "type": "git",
    "url": "https://github.com/GrapeWell/eslint-plugin-auto-optional-chaining"
  },
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "npm-run-all \"lint:*\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint .",
    "test": "mocha tests --recursive",
    "update:eslint-docs": "eslint-doc-generator"
  },
  "dependencies": {
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "eslint": "^9.0.0",
    "@eslint/js": "^9.0.0",
    "eslint-doc-generator": "^2.0.0",
    "eslint-plugin-eslint-plugin": "^6.0.0",
    "eslint-plugin-n": "^17.0.0",
    "mocha": "^11.0.0",
    "npm-run-all2": "^6.1.2"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "peerDependencies": {
    "eslint": ">=9.0.0"
  },
  "license": "MIT"
}
